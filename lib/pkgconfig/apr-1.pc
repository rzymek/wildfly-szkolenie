prefix=/opt/jboss/httpd/httpd
exec_prefix=/opt/jboss/httpd
libdir=/opt/jboss/httpd/lib
APR_MAJOR_VERSION=1
includedir=/opt/jboss/httpd/include/httpd

Name: APR
Description: The Apache Portable Runtime library
Version: 1.5.0
Libs: -L${libdir} -lapr-${APR_MAJOR_VERSION} -lrt -lcrypt  -lpthread
Cflags: -DLINUX -D_REENTRANT -D_GNU_SOURCE -g -O2 -pthread -I${includedir}
